{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "react/function-component-definition":[0,
            {
                "namedComponents": "arrow-function"
            }
        ],
        "arrow-body-style": [0, "always"]
    }
}
